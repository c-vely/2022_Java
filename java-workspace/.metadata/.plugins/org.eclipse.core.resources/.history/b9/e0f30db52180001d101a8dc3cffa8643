package kr.ac.kopo.day01;

/*
	변수 선언 규칙 
	
	자료형 변수명;
	boolean
	char
	byte   -> 
	short
	int    -> 제일 많이 씀
	long   -> 시간 변경시 사용함
	float
	double -> 제일 많이 씀
 */

public class VarMain {

	public static void main(String[] args) {
		
		//정수 123을 저장하기 위한 변수 a 생성 (반드시 초기화해야 함)
		int a;
		a = 123;
		
		
		
		

	}

}
